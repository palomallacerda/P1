#include <stdio.h>

int eh_primo(int numero)
{
 	int rest1 = numero%2;
    int rest2 = numero%3;
    int buraco = 0;
    if (numero == 1)
    {
    	return 0;
    }
    else { 
    if (numero == 1 || numero == 2 || numero == 3)
       {
           return 1;
       }
    else if (rest1 != 0 && rest2 != 0) /*ï¿½ primo */
       {
           return 1;
       }
    else if (rest1 == 0 || rest2 == 0)
       {
     	   return 0;
       }        
    }
}
