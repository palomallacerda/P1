	}
}*/

void ler_colunas(int i, int j, int k, int p, int n,  int matriz[][n], int matriz2[][n])
{
	if (j < n)
	{
		scanf("%d", &matriz[i][j]);
		ler_colunas(i, ++j, k, p, n, matriz, matriz2);
	}
	else
	{
		if (p < n)
		{
			scanf("%d", &matriz2[k][p]);
			ler_colunas(i, j, k, ++p, n, matriz, matriz2);
		}
	}
}

void ler_matriz(int i, int n, int matriz[][n], int matriz2[][n])
{
	if (i < n)
	{
		ler_colunas(i, 0, i, 0, n, matriz, matriz2);
		ler_matriz(++i, n, matriz, matriz2);
	}
}

int main()
{
	int n;
	scanf("%d", &n);

	int matriz[n][n];
	int matriz2[n][n];

	ler_matriz(0, n, matriz, matriz2);

	return 0;
}
