#include <stdio.h>

typedef struct tipos
{
	int id;
	int xp;
	int vida;
	int dano;
	
} monster;


void ler_monstros(int i, int t, monster monstros[]) 
{
	if (i < t)
	{
		scanf("%d %d %d %d", &monstros[i].id, &monstros[i].xp, &monstros[i].vida, &monstros[i].dano);
		ler_monstros(++i, t, monstros);
	}
}

int main()
{
	int t, n, m, vida_ansa, dano_ansa, x, y, k;
	scanf("%d", &t);

	monster monstros[t];

	ler_monstros(0, t, monstros);
	
	scanf("%d%d", &n, &m);

	int mapa[n][m];

	for (int i = 0; i < n; ++i)
	{
		for (int j = 0; j < m; ++j)
			scanf("%d", &mapa[i][j]);
	}

	scanf("%d %d\n %d %d %d", &vida_ansa, &dano_ansa, &x, &y, &k);
	
	int movimentos[k];

	for (int i = 0; i < k; ++i)
		scanf("%d", &movimentos[i]);

	return 0;
}
